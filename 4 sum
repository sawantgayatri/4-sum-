#include <iostream>
using namespace std;

int main()
{
    int n, target;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[100];
    cout << "Enter elements:\n";
    for(int i = 0; i < n; i++)
        cin >> arr[i];

    cout << "Enter target sum: ";
    cin >> target;

    cout << "\nQuadruplets are:\n";

    for(int i = 0; i < n; i++)
    {
        for(int j = i + 1; j < n; j++)
        {
            for(int k = j + 1; k < n; k++)
            {
                for(int l = k + 1; l < n; l++)
                {
                    if(arr[i] + arr[j] + arr[k] + arr[l] == target)
                    {
                        cout << arr[i] << " "
                             << arr[j] << " "
                             << arr[k] << " "
                             << arr[l] << endl;
                    }
                }
            }
        }
    }

    return 0;
}
